{
    "search.exclude": {
      "**/dev": true
    },
    "files.exclude": {
      "**/dev": true,
    },
    "eslint.validate": [
        "javascript",
      "typescript",
      "typescriptreact",
      "svelte"
    ],
    "eslint.lintTask.enable": true,
    "eslint.run": "onSave",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "true"
    },
    "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#62eb83",
      "activityBar.background": "#62eb83",
      "activityBar.foreground": "#15202b",
      "activityBar.inactiveForeground": "#15202b99",
      "activityBarBadge.background": "#8f72ed",
      "activityBarBadge.foreground": "#15202b",
      "commandCenter.border": "#15202b99",
      "sash.hoverBorder": "#62eb83",
      "statusBar.background": "#35e55f",
      "statusBar.foreground": "#15202b",
      "statusBarItem.hoverBackground": "#1acd45",
      "statusBarItem.remoteBackground": "#35e55f",
      "statusBarItem.remoteForeground": "#15202b",
      "titleBar.activeBackground": "#35e55f",
      "titleBar.activeForeground": "#15202b",
      "titleBar.inactiveBackground": "#35e55f99",
      "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#35e55f"
  }